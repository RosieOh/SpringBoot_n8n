{
  "info": {
    "name": "n8n Integration API",
    "description": "Spring Boot n8n Integration API 테스트 컬렉션",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "1. n8n Webhook Trigger",
      "item": [
        {
          "name": "Trigger Webhook (Full)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"webhookPath\": \"/your-webhook-id\",\n  \"data\": {\n    \"message\": \"Hello from Postman\",\n    \"timestamp\": \"{{$timestamp}}\"\n  },\n  \"method\": \"POST\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/n8n/webhook/trigger",
              "host": ["{{baseUrl}}"],
              "path": ["api", "n8n", "webhook", "trigger"]
            },
            "auth": {
              "type": "basic",
              "basic": [
                {
                  "key": "username",
                  "value": "admin"
                },
                {
                  "key": "password",
                  "value": "admin123"
                }
              ]
            }
          }
        },
        {
          "name": "Trigger Webhook (Simple)",
          "request": {
